<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="business.conut">
<class name="Sts_Chat" table="sts_chat">
<id name="id" type="java.lang.Integer">
  <column name="id"></column>  
<generator class="native" />
</id>
<property name="uid" 		column="uid"	 	type="java.lang.Integer"/>
<property name="account"	column="account" 	type="java.lang.String"/>
<property name="nick"		column="nick" 		type="java.lang.String"/>
<property name="absDay" 	column="absDay" 	type="java.lang.Integer"/>
<property name="day" 		column="day" 		type="java.lang.String"/>
<property name="chatMsg" 	column="chatMsg" 	type="java.lang.String"/>
</class>
<!-- 	
	private int id;
	private int uid;
	private int nick;
	private int absDay;
	private String day;
	private String chatMsg; -->

</hibernate-mapping>

