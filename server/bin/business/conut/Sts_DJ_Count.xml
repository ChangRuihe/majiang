<property name="timeDay" 		column="timeDay"	 	type="java.lang.String"/>
<property name="timeMonth" 		column="timeMonth"	 	type="java.lang.String"/>

<property name="Time_1" 	column="Time_1"	 	type="java.lang.Integer"/>
<property name="Time_2" 	column="Time_2"	 	type="java.lang.Integer"/>
<property name="Time_3" 	column="Time_3"	 	type="java.lang.Integer"/>
