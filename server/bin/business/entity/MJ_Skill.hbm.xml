 <column name="animeType" />
</property>
<property name="desc0"  type="java.lang.String">
 <column name="desc0" />
</property>
<property name="condition0"  type="java.lang.String">
 <column name="condition0" />
</property>
<property name="attr0"  type="java.lang.Integer">
 <column name="attr0" />
</property>
</class>
</hibernate-mapping>
