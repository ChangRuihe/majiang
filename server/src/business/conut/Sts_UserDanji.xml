<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="business.conut">
<class name="Sts_UserDanji" table="sts_userdanji">
<id name="id" type="java.lang.Integer">
  <column name="id"></column>  
<generator class="native" />
</id>
<property name="absDay" 	column="absDay"	 	type="java.lang.Integer"/>
<property name="timeDay" 		column="timeDay"	 	type="java.lang.String"/>
<property name="timeMonth" 		column="timeMonth"	 	type="java.lang.String"/>
<property name="uid" 		column="uid"	 	type="java.lang.Integer"/>
<property name="account" 	column="account" 	type="java.lang.String"/>
<property name="nick"		column="nick" 		type="java.lang.String"/>

<property name="danjiId" 		column="danjiId"	 	type="java.lang.Integer"/>
<property name="startFlag" 		column="startFlag"	 	type="java.lang.Integer"/>
<property name="endFlag" 		column="endFlag"	 	type="java.lang.Integer"/>

<property name="startTime"		column="startTime" 		type="java.lang.String"/>
<property name="endTime"		column="niendTimeck" 		type="java.lang.String"/>

</class>
