<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="business.conut">
<class name="Sts_MJDevice" table="sts_mjdevice">
<id name="id" type="java.lang.Integer">
  <column name="id"></column>  
<generator class="native" />
</id>
<property name="absDay" 		column="absDay"	 		type="java.lang.Integer"/>
<property name="time1"			column="time1" 			type="java.lang.String"/>
<property name="time2"			column="time2" 			type="java.lang.String"/>
<property name="newAdd" 		column="newAdd" 		type="java.lang.Integer"/>
<property name="duliDevice" 	column="duliDevice" 	type="java.lang.Integer"/>
<property name="openDevice" 	column="openDevice" 	type="java.lang.Integer"/>
</class>
<!-- 	
	private int absDay;
	private String time1;  //日
	private String time2;  //月
	private int newAdd;
	private int duliDevice;
	private int openDevice;
	
	-->

</hibernate-mapping>

