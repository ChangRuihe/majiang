<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="business.conut">
<class name="Sts_UserOperate" table="sts_useroperate">
<id name="id" type="java.lang.Integer">
  <column name="id"></column>  
<generator class="native" />
</id>
<property name="uid" 		column="uid"	 	type="java.lang.Integer"/>
<property name="absDay" 	column="absDay"	 	type="java.lang.Integer"/>
<property name="day"		column="day" 		type="java.lang.String"/>
<property name="operate"	column="operate" 	type="java.lang.String"/>
</class>
<!-- 	
	private int id;
	private int uid;
	private int absDay;
	private String day;
	private String operate;
-->
</hibernate-mapping>

